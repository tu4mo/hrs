{
  "name": "hrs-cli",
  "version": "0.1.0-alpha.3",
  "description": "Simple CLI for tracking your work day",
  "main": "dist/hrs.js",
  "bin": {
    "hrs": "./dist/hrs.js"
  },
  "scripts": {
    "format": "prettier --no-semi --single-quote --write \"**/*.{ts,md}\"",
    "prepublish": "tsc",
    "start": "ts-node src/hrs",
    "test": "tslint \"src/**/*.ts\" && jest \"src\"",
    "test:coverage": "jest --coverage \"src\"",
    "test:watch": "jest --watch \"src\""
  },
  "author": "tu4mo",
  "license": "ISC",
  "dependencies": {
    "date-fns": "^1.30.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^2.9.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tu4mo/hrs.git"
  },
  "bugs": {
    "url": "https://github.com/tu4mo/hrs/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "homepage": "https://github.com/tu4mo/hrs",
  "preferGlobal": true
}
